<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>4.4</version>
    <date>2025-07-15T09:04:49Z</date>
    <groups>
        <group>
            <name>Templates/Applications</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>Template APP EDAC</template>
            <name>Template APP EDAC</name>
            <description>Read EDAC data (ECC errors)</description>
            <groups>
                <group>
                    <name>Templates/Applications</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>EDAC</name>
                </application>
                <application>
                    <name>EDAC - Raw</name>
                </application>
            </applications>
            <items>
                <item>
                    <name>EDAC JSON</name>
                    <key>edac</key>
                    <delay>10m</delay>
                    <history>0</history>
                    <trends>0</trends>
                    <value_type>TEXT</value_type>
                    <applications>
                        <application>
                            <name>EDAC - Raw</name>
                        </application>
                    </applications>
                </item>
            </items>
            <discovery_rules>
                <discovery_rule>
                    <name>Discover DIMM</name>
                    <type>DEPENDENT</type>
                    <key>edac.discovery</key>
                    <delay>0</delay>
                    <item_prototypes>
                        <item_prototype>
                            <name>{#DIMM_MC} - {#DIMM_NAME} - Errors Correctable</name>
                            <type>DEPENDENT</type>
                            <key>edac.ce.[{#DIMM_MC_NAME}]</key>
                            <delay>0</delay>
                            <history>31d</history>
                            <applications>
                                <application>
                                    <name>EDAC</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.['{#DIMM_MC}'].dimms.['{#DIMM_NAME}'].dimm_ce_count</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>edac</key>
                            </master_item>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&gt;{$EDAC_CE_THRESH}</expression>
                                    <name>{#DIMM_MC}  - {#DIMM_NAME} - Correctable errors detected: {ITEM.LASTVALUE}</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>{#DIMM_MC} - {#DIMM_NAME} - Location</name>
                            <type>DEPENDENT</type>
                            <key>edac.location[{#DIMM_MC_NAME}]</key>
                            <delay>0</delay>
                            <history>1h</history>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>EDAC</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.['{#DIMM_MC}'].dimms.['{#DIMM_NAME}'].dimm_location</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>edac</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#DIMM_MC} - {#DIMM_NAME} - Size</name>
                            <type>DEPENDENT</type>
                            <key>edac.size[{#DIMM_MC_NAME}]</key>
                            <delay>0</delay>
                            <history>1h</history>
                            <trends>0</trends>
                            <units>B</units>
                            <applications>
                                <application>
                                    <name>EDAC</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.['{#DIMM_MC}'].dimms.['{#DIMM_NAME}'].size</params>
                                </step>
                                <step>
                                    <type>MULTIPLIER</type>
                                    <params>1048576</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>edac</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#DIMM_MC} - {#DIMM_NAME} - Errors Uncorrectable</name>
                            <type>DEPENDENT</type>
                            <key>edac.ue[{#DIMM_MC_NAME}]</key>
                            <delay>0</delay>
                            <history>31d</history>
                            <applications>
                                <application>
                                    <name>EDAC</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.['{#DIMM_MC}'].dimms.['{#DIMM_NAME}'].dimm_ue_count</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>edac</key>
                            </master_item>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&gt;{$EDAC_UE_THRESH}</expression>
                                    <name>{#DIMM_MC} - {#DIMM} - Uncorrectable errors detected: {ITEM.LASTVALUE}</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                    <master_item>
                        <key>edac</key>
                    </master_item>
                    <lld_macro_paths>
                        <lld_macro_path>
                            <lld_macro>{#DIMM_MC_NAME}</lld_macro>
                            <path>$.mc_name</path>
                        </lld_macro_path>
                        <lld_macro_path>
                            <lld_macro>{#DIMM_MC}</lld_macro>
                            <path>$.mc</path>
                        </lld_macro_path>
                        <lld_macro_path>
                            <lld_macro>{#DIMM_NAME}</lld_macro>
                            <path>$.name</path>
                        </lld_macro_path>
                    </lld_macro_paths>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <params>$..dimms.*</params>
                        </step>
                    </preprocessing>
                </discovery_rule>
                <discovery_rule>
                    <name>Discover MC</name>
                    <type>DEPENDENT</type>
                    <key>edac.mc_discovery</key>
                    <delay>0</delay>
                    <item_prototypes>
                        <item_prototype>
                            <name>{#MC} - Errors Correctable</name>
                            <type>DEPENDENT</type>
                            <key>edac.ce.[{#MC}]</key>
                            <delay>0</delay>
                            <history>31d</history>
                            <applications>
                                <application>
                                    <name>EDAC</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.['{#MC}'].info.ce_count</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>edac</key>
                            </master_item>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&gt;{$EDAC_CE_THRESH}</expression>
                                    <name>{#MC} - Correctable errors detected: {ITEM.LASTVALUE}</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                        <item_prototype>
                            <name>{#MC} - Name</name>
                            <type>DEPENDENT</type>
                            <key>edac.mc_name[{#MC}]</key>
                            <delay>0</delay>
                            <history>1h</history>
                            <trends>0</trends>
                            <value_type>TEXT</value_type>
                            <applications>
                                <application>
                                    <name>EDAC</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.['{#MC}'].info.mc_name</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>edac</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#MC} - Size</name>
                            <type>DEPENDENT</type>
                            <key>edac.size[{#MC}]</key>
                            <delay>0</delay>
                            <history>31d</history>
                            <applications>
                                <application>
                                    <name>EDAC</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.['{#MC}'].info.size_mb</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>edac</key>
                            </master_item>
                        </item_prototype>
                        <item_prototype>
                            <name>{#MC} - Errors Uncorrectable</name>
                            <type>DEPENDENT</type>
                            <key>edac.ue[{#MC}]</key>
                            <delay>0</delay>
                            <history>31d</history>
                            <applications>
                                <application>
                                    <name>EDAC</name>
                                </application>
                            </applications>
                            <preprocessing>
                                <step>
                                    <type>JSONPATH</type>
                                    <params>$.['{#MC}'].info.ue_count</params>
                                </step>
                            </preprocessing>
                            <master_item>
                                <key>edac</key>
                            </master_item>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last()}&gt;{$EDAC_UE_THRESH}</expression>
                                    <name>{#MC} - Uncorrectable errors detected: {ITEM.LASTVALUE}</name>
                                    <priority>HIGH</priority>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                    <master_item>
                        <key>edac</key>
                    </master_item>
                    <lld_macro_paths>
                        <lld_macro_path>
                            <lld_macro>{#MC}</lld_macro>
                            <path>$.info.mc</path>
                        </lld_macro_path>
                    </lld_macro_paths>
                    <preprocessing>
                        <step>
                            <type>JSONPATH</type>
                            <params>$.*</params>
                        </step>
                    </preprocessing>
                </discovery_rule>
            </discovery_rules>
            <macros>
                <macro>
                    <macro>{$EDAC_CE_THRESH}</macro>
                    <value>0</value>
                </macro>
                <macro>
                    <macro>{$EDAC_UE_THRESH}</macro>
                    <value>0</value>
                </macro>
            </macros>
        </template>
    </templates>
</zabbix_export>
